#include<iostream>
using namespace std;
int A[64][64];
int n;
int  compress(int i, int j, int N, int threshold)
{	if(N == 1)
	return A[i][j];	
	else
	{	int c1 = 0, c0 = 0;
		//Count of 0's and 1's
		for(int p = i; p < i + N; ++p)
		{  for(int q = j; q < j + N; ++q)
		    {
		        if(A[p][q] == 1)
		           ++c1;
		        else 
		           ++c0;
		    }
		}
		c1 = c1 * 100 / (N * N);
		c0 = c0 * 100 / (N * N);
		
		//First Quadrant
		int type = compress(i, j, N / 2, threshold);
		
		if(type != -1)
		{
			for(int p= i; p < i + N / 2; ++p)
			 for(int q = j; q< j + N / 2; ++q)
				 A[p][q] = type;
		}
		
		//Second Quadrant
		type = compress(i + N / 2, j + N / 2, N / 2, threshold);
		
		if(type != -1)
		{ for(int p = i + N / 2; p < i + N; ++p)
			for(int q = j + N / 2; q < j + N; ++q)
		 		 A[p][q] = type;	
		}
			
		//Third Quadrant
			type = compress(i, j + N / 2, N / 2, threshold);
		
		if(type != -1)
		{  for(int p = i; p < i + N / 2; ++p)
			  for(int q = j + N / 2; q < j + N; ++q)
				 A[p][q] = type;	
		}
		
		//4th Quadrant
		type = compress(i + N / 2, j, N / 2, threshold);
		
		if(type != -1)
		{  for(int p = i + N / 2; p < i + N; ++p)
			 for(int q = j; q < j + N / 2; ++q)
			   A[p][q] = type;	
		}
	
		
		if(N == n)             //Ending Part
		{  if(c1 >= threshold)
		    { for(int p = i; p < i + N; ++p)
		       for(int q = j; q < j + N; ++q)
		        	A[p][q] = 1;	
		    }
		    else if(c0 >= threshold)
		    { for(int p = i; p < i + N; ++p)
			    for(int q = j; q < j + N; ++q)
		    	   A[p][q] = 0;
		    }
		    return -1;
		}
		
		else
		{   if(c1 >= threshold) // more than threshold is 1
    		   return 1;
    		else if(c0 >= threshold)//more than threshould is 0
    		   return 0;
       		else
       		   return -1;
		}
	}
}
int main()
{  
	int f=1,num = 1;
	
	while(f)
	{
		int N, threshold;
		cin>>N>>threshold;
		n = N;
		if(N == 0)
	          f=0;
		if(f)
		{for(int i = 0; i < N; ++i)
		 for(int j = 0; j < N; ++j)
			cin>>A[i][j];
		compress(0, 0, N, threshold);
		cout<<"Image "<<num<<":"<<endl;
		
		for(int i = 0; i < N; ++i)
		{   for(int j = 0; j < N; ++j)
			   cout<<A[i][j]<<"  ";
		    cout<<endl;
		}
		num+=1;
	}}
	return 0;
}
